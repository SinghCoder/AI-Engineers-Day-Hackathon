{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "npm: compile",
      "env": {
        "LANGFUSE_PUBLIC_KEY": "lf-pk-qordinate-ai",
        "LANGFUSE_SECRET_KEY": "lf-sk-qordinate-ai",
        "LANGFUSE_BASE_URL": "http://localhost:3001"
      }
    }
  ]
}
